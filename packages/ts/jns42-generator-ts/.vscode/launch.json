{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "run file",
      "program": "${file}",
      "args": [],
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "watch",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "package file",
      "program": "${workspaceFolder}/src/program.ts",
      "args": [
        "package",
        "${file}",
        "--package-directory",
        ".package/file",
        "--package-name",
        "file",
        "--package-version",
        "0.0.0"
      ],
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/out/**/*.js"],
      "preLaunchTask": "watch"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "package draft-2020-12",
      "program": "${workspaceFolder}/src/program.ts",
      "args": [
        "package",
        "https://json-schema.org/draft/2020-12/schema",
        "--package-directory",
        ".package/schema/draft-2020-12",
        "--package-name",
        "draft-2020-12",
        "--package-version",
        "0.0.0"
