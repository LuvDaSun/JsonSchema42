{
  "name": "jns42-generator",
  "version": "0.8.9",
  "sideEffects": false,
  "description": "generate interfaces and validators from a json schema",
  "type": "module",
  "bin": {
    "jns42-generator": "out/program.js"
  },
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "out/*",
    "src/*"
  ],
  "scripts": {
    "build": "tsc --build && chmod +x out/program.js",
    "test": "node --test ./out/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/JsonSchema42.git"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/JsonSchema42/issues"
  },
  "keywords": [
    "json",
    "schema",
    "generator"
  ],
  "homepage": "https://www.JsonSchema42.org",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "typescript": "^5.3.2",
    "yaml": "^2.3.4"
  },
  "dependencies": {
    "schema-draft-04": "file://../schema-draft-04",
    "schema-draft-2020-12": "file://../schema-draft-2020-12",
    "schema-intermediate": "file://../schema-intermediate",
    "@types/node": "^20.10.0",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "tslib": "^2.6.2",
    "type-fest": "^4.6.0",
    "typescript": "^5.3.2",
    "yargs": "^17.7.2"
  },
  "bundledDependencies": [
    "schema-draft-04",
    "schema-draft-2020-12",
    "schema-intermediate"
  ]
}
