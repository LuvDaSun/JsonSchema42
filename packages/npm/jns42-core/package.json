{
  "name": "@jns42/core",
  "version": "0.7.11",
  "sideEffects": false,
  "description": "core library",
  "type": "module",
  "module": "./src/main.js",
  "types": "./src/main.d.ts",
  "exports": {
    ".": {
      "import": "./src/main.js",
      "types": "./src/main.d.ts"
    }
  },
  "files": [
    "./src/**",
    "./dist/**"
  ],
  "scripts": {
    "prepack": "npm run build",
    "pretest": "npm run build",
    "build": "just build-npm-jns42-core",
    "test": "node --test ./src/**/*.test.js",
    "generate": "jco transpile --instantiation async --out-dir dist $npm_config_local_prefix/target/wasm32-unknown-unknown/release/jns42_core.component.wasm"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/JsonSchema42.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/LuvDaSun"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/JsonSchema42/issues"
  },
  "keywords": [
    "json",
    "schema",
    "core"
  ],
  "homepage": "https://www.JsonSchema42.org",
  "devDependencies": {
    "@bytecodealliance/jco": "^1.11.2"
  },
  "engines": {
    "node": ">=18"
  }
}
