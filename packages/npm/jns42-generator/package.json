{
  "name": "jns42-generator",
  "version": "0.14.0",
  "sideEffects": false,
  "description": "generate interfaces and validators from a json schema",
  "type": "module",
  "bin": {
    "jns42-generator": "bin/jns42-generator"
  },
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "bin/**",
    "out/**",
    "src/**"
  ],
  "scripts": {
    "prepack": "tsc --build",
    "pretest": "tsc --build",
    "build": "tsc --build",
    "clean": "rm -rf ./out && tsc --build --clean",
    "test": "node --test ./out/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/JsonSchema42.git"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/JsonSchema42/issues"
  },
  "keywords": [
    "json",
    "schema",
    "generator"
  ],
  "homepage": "https://www.JsonSchema42.org",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "c8": "^9.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@jns42/oas-v3-0": "^0.13.7",
    "@jns42/oas-v3-1": "^0.13.7",
    "@jns42/schema-draft-04": "^0.13.7",
    "@jns42/schema-draft-2020-12": "^0.13.7",
    "@jns42/schema-intermediate": "^0.13.7",
    "@jns42/schema-oas-v3-1": "^0.13.7",
    "@jns42/swagger-v2": "^0.13.7",
    "@types/node": "^20.11.20",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "entities": "^4.5.0",
    "hash.js": "^1.1.7",
    "tslib": "^2.6.2",
    "type-fest": "^4.9.0",
    "yaml": "^2.4.0",
    "yargs": "^17.7.2"
  }
}
