{
  "name": "jns42-generator",
  "version": "0.14.3",
  "sideEffects": false,
  "description": "generate interfaces and validators from a json schema",
  "type": "module",
  "main": "./bundled/main.cjs",
  "module": "./bundled/main.js",
  "types": "./types/main.d.ts",
  "exports": {
    ".": {
      "require": "./bundled/main.cjs",
      "import": "./bundled/main.js",
      "types": "./types/main.d.ts"
    }
  },
  "files": [
    "./types/**",
    "./bundled/**"
  ],
  "bin": {
    "jns42-generator": "bundled/program.js"
  },
  "scripts": {
    "prepack": "./scripts/build.js",
    "pretest": "./scripts/build.js",
    "build": "./scripts/build.js",
    "clean": "./scripts/clean.js",
    "test": "node --test ./transpiled/**/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/LuvDaSun/JsonSchema42.git"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuvDaSun/JsonSchema42/issues"
  },
  "keywords": [
    "json",
    "schema",
    "generator"
  ],
  "homepage": "https://www.JsonSchema42.org",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "c8": "^9.1.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@jns42/oas-v3-0": "^0.14.3",
    "@jns42/oas-v3-1": "^0.14.3",
    "@jns42/schema-draft-04": "^0.14.3",
    "@jns42/schema-draft-2020-12": "^0.14.3",
    "@jns42/schema-intermediate": "^0.14.3",
    "@jns42/schema-oas-v3-1": "^0.14.3",
    "@jns42/swagger-v2": "^0.14.3",
    "@types/node": "^20.11.25",
    "@types/yargs": "^17.0.32",
    "camelcase": "^8.0.0",
    "entities": "^4.5.0",
    "hash.js": "^1.1.7",
    "tslib": "^2.6.2",
    "type-fest": "^4.9.0",
    "yaml": "^2.4.1",
    "yargs": "^17.7.2"
  }
}
